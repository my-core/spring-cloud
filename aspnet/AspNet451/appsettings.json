{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  //注册到spring cloud的服务名
  "spring": {
    "application": {
      "name": "AspNet451"
    }
  },
  //spring cloud的客户端配置
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/" //注册中心地址（本机）
      //"serviceUrl": "http://192.168.2.118:8761/eureka/" //注册中心地址（测试环境）
    },
    "instance": {
      "port": 8040 //服务实例端口，这里要与程序启动的端口一致
    }
  }
} 
