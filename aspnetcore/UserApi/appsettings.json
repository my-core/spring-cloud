{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  //注册到spring cloud的服务名
  "spring": {
    "application": {
      "name": "User"
    }
  },
  //spring cloud的客户端配置
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/" //注册中心地址
    },
    "instance": {
      "port": 8010 //服务实例端口，这里要与WebHostBuilder.UseUrls()设置端口一致
    }
  }
} 
